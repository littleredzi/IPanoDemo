<krpano version="1.18" title="深圳罗湖" onstart="startup();" showerrors="true">

    <include url="vtourskin.xml" />

    <!-- set skin settings: bingmaps? gyro? thumbnails? tooltips? -->
    <skin_settings maps="false"
                   maps_zoombuttons="false"
                   gyro="true"
                   title="true"
                   thumbs="true"
                   thumbs_opened="false"
                   thumbs_width="120" thumbs_height="80" thumbs_padding="10"
                   thumbs_text="true"
                   thumbs_dragging="true"
                   thumbs_onhoverscrolling="true"
                   thumbs_scrollbuttons="false"
                   thumbs_scrollindicator="false"
                   thumbs_loop="false"
                   tooltips_thumbs="true"
                   tooltips_hotspots="false"
                   tooltips_mapspots="false"
                   loadscene_flags="MERGE"
                   loadscene_blend="BLEND(0.5)"
                   controlbar_offset="50"
    />
    <!-- set optional skin logo url -->
    <layer name="skin_logo" url="" scale="0.25" opened_onclick="openurl('...',_blank);" />

    <action name="startup">
        if(startscene === null, copy(startscene,scene[0].name));
        set(skin_settings.scene_count, get(number));
        set(scene[0].title,get(name0));
        set(scene[1].title,get(name1));
        set(scene[2].title,get(name2));
        set(scene[3].title,get(name3));
        set(scene[4].title,get(name4));
        set(scene[5].title,get(name5));
        set(scene[6].title,get(name6));
        set(scene[7].title,get(name7));
        set(scene[8].title,get(name8));
        set(scene[9].title,get(name9));
        set(scene[10].title,get(name10));
        set(scene[11].title,get(name11));
        set(scene[12].title,get(name12));
        set(scene[13].title,get(name13));
        set(scene[14].title,get(name14));
        set(scene[15].title,get(name15));
        set(scene[16].title,get(name16));
        set(scene[17].title,get(name17));
        set(scene[18].title,get(name18));
        set(scene[19].title,get(name19));
        set(scene[20].title,get(name20));
        set(scene[21].title,get(name21));
        set(scene[22].title,get(name22));
        set(scene[23].title,get(name23));
        set(scene[24].title,get(name24));
        set(scene[25].title,get(name25));
        set(scene[26].title,get(name26));
        set(scene[27].title,get(name27));
        set(scene[28].title,get(name28));
        set(scene[29].title,get(name29));
        loadscene(get(startscene), null, MERGE);
        skin_showthumbs();
    </action>

    <data name="bottom_html">
        <p>速腾聚创</p>
    </data>

    <!-- 底部广告图 -->
    <hotspot name="bottom_background"
             url="%SWFPATH%/images/bottom.png"
             enabled="true"
             keep="true"
             handcursor="false"
             ath="0" atv="90" rx="0.0" ry="0.0" rz="0.0"
             width="360" height="360"
             distorted="true"
             ondown="skin_view_change_with_hotspot();"
             />
    <hotspot name="bottom_trademark"
             url="%SWFPATH%/plugins/textfield.swf"
             css="text-align:center; color:#494949; font-family:黑体; text-align:center;  font-size:40px;"
             enabled="true"
             keep="true"
             shadowangle="45" shadowrange="4.0"
             handcursor="false"
             background="false"
             ath="0" atv="90" rx="0.0" ry="0.0" rz="0.0"
             distorted="true"
             html="data:bottom_html"
             />

    <!-- 顶部广告图 -->
    <hotspot name="top_trademark" 
             type="image"
             waittime="1"
             keep="true"
             url="%SWFPATH%/images/logo_ie.png"
             devices="all"
             handcursor="false"
             ath="0" atv="-90" rx="0.0" ry="0.0" rz="0.0"
             width="360" height="360"
             distorted="true"
             ondown="skin_view_change_with_hotspot();"
             />

